{
  "extends": "../../tsconfig.json",
  // Specifies a list of type declarations to be included in auto type acquisition. Ex. ["jquery", "lodash"]
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
     "src/**/*.tsx",
     "src/**/*.vue",
     ".src/env.d.ts"
    ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@": ["src"],
      "@/*": ["src/*"]
    }
  }
}